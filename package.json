{
  "name": "gov-csr-portal",
  "version": "1.0.0",
  "description": "Government/CSR Partners Portal for Skilling Programmes",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "cd frontend && npm run build",
    "start": "cd backend && npm start",
    "test": "cd backend && npm test"
  },
  "keywords": ["government", "csr", "skilling", "mongodb", "react", "node"],
  "author": "Government CSR Portal Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
